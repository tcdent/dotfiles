{
  "statusLine": {
    "type": "command",
    "command": "printf '%s:%s ' \"$(hostname -s)\" \"$(pwd | sed \"s|^$HOME|~|\")\""
  },
  "alwaysThinkingEnabled": true,
  "permissions": {
    "allow": [
      "WebFetch(domain:*)",
      "Bash(find :*)"
    ],
    "ask": [
      "Bash(rm:*)"
    ],
    "deny": []
  }
}
